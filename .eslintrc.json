{
  "extends": ["eslint:recommended", "prettier", "react-app", "react-app/jest"],
  "plugins": ["prettier", "eslint-plugin-prettier"],
  "rules": {
    "camelcase": ["error", { "properties": "never" }],
    "prettier/prettier": "off",
    "eqeqeq": ["error", "always"],
    "no-unused-vars": ["error"]
  }
}
